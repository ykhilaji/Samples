<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" name="GuiceRedisCrud" packages="">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="[%level][%logger][%d][pid: %pid]: %m%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="debug">
            <AppenderRef ref="Console"/>
        </Root>
        <Logger name="org.apache" additivity="false" level="info">
            <AppenderRef ref="Console"/>
        </Logger>
    </Loggers>
</Configuration>